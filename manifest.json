{
   "name": "WebCov",
   "version": "1.0",
   "description": "Chrome extension for visualizing test coverage",
   "permissions": ["activeTab", "scripting", "declarativeContent", "storage", "file:///*"],
   "host_permissions": ["https://*/*", "http://*/*"],
   "action": {
     "default_popup": "popup.html",
     "default_icon": {
       "16": "images/get_started16.png",
       "32": "images/get_started32.png",
       "48": "images/get_started48.png",
       "128": "images/get_started128.png"
     }
   },
   "icons": {
     "16": "images/get_started16.png",
     "32": "images/get_started32.png",
     "48": "images/get_started48.png",
     "128": "images/get_started128.png"
   },
   "manifest_version": 3
}
